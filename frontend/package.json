{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "main": "src/index.tsx",
  "scripts": {
    "sync-shared-constants": "node ../scripts/sync-shared-constants.js",
    "generate:theme": "node scripts/generate-theme.js",
    "predev": "npm run sync-shared-constants && npm run generate:theme",
    "dev": "vite",
    "prestart": "npm run sync-shared-constants && npm run generate:theme",
    "start": "vite",
    "prebuild": "npm run sync-shared-constants && npm run generate:theme",
    "build": "vite build",
    "preview": "vite preview",
    "postinstall": "npm run sync-shared-constants",
    "pretest": "npm run sync-shared-constants",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 110",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "type-check": "tsc --noEmit",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test tests/e2e/smoke --grep @smoke",
    "test:e2e:regression": "playwright test tests/e2e/regression --grep @regression",
    "test:e2e:all": "playwright test",
    "test:e2e:setup": "playwright test tests/e2e/setup",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report test-results/e2e-report",
    "validate:constants": "node ../scripts/validate-constants.js",
    "validate:shared-constants": "node ../scripts/validate-shared-constants-usage.js",
    "validate:data-testid": "node ../scripts/validate-data-testid-consistency.js",
    "validate:barrel-imports": "node ../scripts/validate-barrel-imports.js",
    "validate:console-cleanup": "node ../scripts/validate-console-cleanup.js",
    "validate:all": "node ../scripts/validate-all-automation.js",
    "validate:quick": "npm run validate:constants && npm run validate:shared-constants",
    "fix:hardcoded-strings": "node ../scripts/fix-hardcoded-strings.js",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "@types/exceljs": "^0.5.3",
    "@types/file-saver": "^2.0.7",
    "@types/react-csv": "^1.1.10",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "i18next": "^23.10.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.508.0",
    "react": "^18.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^13.2.2",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.17",
    "xlsx": "^0.18.5",
    "zod": "^3.25.23",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.16",
    "@types/react": "19.1.5",
    "@types/react-dom": "18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__react": "^10.0.1",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "mailslurp-client": "^15.23.1",
    "msw": "^2.8.4",
    "nodemailer": "^7.0.3",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "react-refresh": "^0.17.0",
    "tailwindcss": "^3.4.16",
    "typescript": "4.9.5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "overrides": {
    "@types/react": "19.1.5",
    "@types/react-dom": "18.3.0",
    "typescript": "4.9.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@testing-library/react": {
      "@types/react": "18.3.3",
      "@types/react-dom": "18.3.0"
    },
    "zustand": {
      "@types/react": "18.3.3"
    },
    "@types/react-router-dom": {
      "@types/react": "18.3.3"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "npm run validate:constants",
      "npm run validate:shared-constants"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix", 
      "prettier --write"
    ],
    "src/**/*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
